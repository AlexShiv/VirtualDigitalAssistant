<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Events.
    -->
    <changeSet id="20200121130634-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="client_id"
                                 baseTableName="events"
                                 constraintName="fk_events_client_id"
                                 referencedColumnNames="id"
                                 referencedTableName="clients"/>

        <addForeignKeyConstraint baseColumnNames="event_types_id"
                                 baseTableName="events"
                                 constraintName="fk_events_event_types_id"
                                 referencedColumnNames="id"
                                 referencedTableName="event_types"/>

    </changeSet>
</databaseChangeLog>
